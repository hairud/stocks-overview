{
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "target": "ES5",
    "allowJs": false,
    "sourceMap": true,
    "module": "ESNext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "#assets/*": ["src/assets/*"],
      "#assets": ["src/assets"],
      "#components/*": ["src/components/*"],
      "#components": ["src/components"],
      "#constants/*": ["src/constants/*"],
      "#constants": ["src/constants"],
      "#helpers/*": ["src/helpers/*"],
      "#helpers": ["src/helpers"],
      "#pages/*": ["src/pages/*"],
      "#pages": ["src/pages"],
      "#store/*": ["src/store/*"],
      "#store": ["src/store"],
      "#types/*": ["src/types/*"],
      "#types": ["src/types"],
      "#utils/*": ["src/utils/*"],
      "#utils": ["src/utils"]
    }
  },
  "exclude": ["node_modules", "dist", "config"]
}
